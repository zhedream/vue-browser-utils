{
  "compilerOptions": {
    "strict": false,
    "jsx": "preserve",
    "skipLibCheck": true,
    "allowJs": true,
    "allowSyntheticDefaultImports": true,
    "moduleResolution": "node",
    "baseUrl": ".",
    "noEmit": true,
    // echarts.ts  两个 ts 问题
    "allowImportingTsExtensions": true,
    // [].find
    "target": "ES2016",
    "paths": {
      "@/*": ["./*"]
    }
  }
}
